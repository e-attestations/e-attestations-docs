    <span class="nv">$response</span> <span class="o">=</span> <span class="nv">$client</span><span class="o">-&gt;</span><span class="na">request</span><span class="p">(</span><span class="s1">'POST'</span><span class="p">,</span><span class="s1">'https://rs.e-attestations.com/api/v1/account/{accountId}/dossiers/{dossierId}/reactivate'</span><span class="p">,</span> <span class="k">array</span><span class="p">(</span>
